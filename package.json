{
  "name": "angular-material-docs",
  "displayName": "Angular Material documentation",
  "description": "",
  "publisher": "RobinDeblauwe",
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "icon": "src/resources/angular.png",
  "repository": {
    "url": "https://github.com/RobinDBL/angular-material-docs"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:angular-material-docs.search-angular-material-docs",
    "onCommand:angular-material-docs.helloWorld",
    "onView:angular-material-search"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "angular-material-docs",
          "title": "Angular Material documentation",
          "icon": "src/resources/angular.svg"
        }
      ]
    },
    "views": {
      "angular-material-docs": [
        {
          "type": "webview",
          "id": "angular-material-search",
          "name": "Angular Material Documentation search",
          "icon": "src/resources/angular.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "angular-material-docs.search-angular-material-docs",
        "title": "Search angular material docs"
      },
      {
        "command": "angular-material-docs.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "rollup -c && tsc -p ./",
    "watch": "concurrently \"rollup -c -w\" \"tsc -watch -p ./\"",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/glob": "^7.2.0",
    "@types/jsdom": "^20.0.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "concurrently": "^7.3.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.1",
    "svelte-preprocess": "^4.10.7",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "rollup": "^2.78.1",
    "axios": "^0.27.2",
    "cheerio": "^1.0.0-rc.12",
    "jsdom": "^20.0.0",
    "puppeteer": "^16.2.0"
  }
}
